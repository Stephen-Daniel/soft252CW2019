package LoginSystem;

import java.text.SimpleDateFormat;

public class Date {
	
	
	public void Date() {
		
	
	showTime();
		
	
	}
	
	public String showDate(String date) {
		
		Date d = new Date();
		SimpleDateFormat s = new SimpleDateFormat("dd-MM-yyyy");
		date = s.format(d);
		return date;
	}
	public void showTime() {
		
	}


	
	
}
