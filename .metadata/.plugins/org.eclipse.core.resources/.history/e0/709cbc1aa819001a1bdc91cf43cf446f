package LoginSystem;

import java.text.SimpleDateFormat;

public class Date {
	private String dateNow;
	
	public void Date() {
		
	
	showTime();
		
	
	}
	
	public String showDate(String dateNow) {
		
		Date d = new Date();
		SimpleDateFormat s = new SimpleDateFormat("yyyy-MM-dd");
		dateNow = s.format(d);
		return dateNow;
	}
	public void showTime() {
		
	}


	
	
}
