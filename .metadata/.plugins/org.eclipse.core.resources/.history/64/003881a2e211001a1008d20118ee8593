package LoginSystem;
import java.io.FileWriter;
import java.io.IOException;
import java.util.Scanner;

public class uniqueNumber {
	
	public int number;
	
	public uniqueNumber()
	{
		@SuppressWarnings("resource")
		Scanner readNumber = new Scanner("savedNumber");
		number = Integer.parseInt(readNumber.next());
		number++;
        try {
            FileWriter writer = new FileWriter("MyFile.txt", true);
            writer.write(number);
            
            writer.close();
        } catch (IOException e) {
            e.printStackTrace();
        }
 
    
		
		return number;
	}
	
	
	

}
