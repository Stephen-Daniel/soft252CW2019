package LoginSystem;
import java.io.FileWriter;
import java.io.IOException;
import java.util.Scanner;

public class uniqueNumber {
	
	public int number;
	
	public int uniqueNumber()
	{
		@SuppressWarnings("resource")
		Scanner readNumber = new Scanner("nextNumber.txt");
		String text = readNumber.next();
		number = Integer.parseInt(text);
		number++;
        try {
            FileWriter writer = new FileWriter("nextNumber.txt", true);
            writer.write(number);
            
            writer.close();
        } catch (IOException e) {
            e.printStackTrace();
        }
 
    
		
		return number;
	}
	
	
	

}
